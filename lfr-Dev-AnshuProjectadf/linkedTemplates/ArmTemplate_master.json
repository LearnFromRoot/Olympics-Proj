{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lfr-Dev-AnshuProjectadf"
		},
		"ls_log_files_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_log_files'"
		},
		"ls_source_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_source_adlsgen2'"
		},
		"ls_source_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lfranshuproject.dfs.core.windows.net/"
		},
		"ls_source_http_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "lfr-Dev-AnshuProjectadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_log_files_connectionString": {
						"value": "[parameters('ls_log_files_connectionString')]"
					},
					"ls_source_adlsgen2_accountKey": {
						"value": "[parameters('ls_source_adlsgen2_accountKey')]"
					},
					"ls_source_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_source_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_source_http_git_properties_typeProperties_url": {
						"value": "[parameters('ls_source_http_git_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}